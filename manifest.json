{
  "manifest_version": 2,
  "name": "Oh Really",
  "description": "",
  "version": "0.1",
  "author": "Anatoli Makarevich",
  "icons": {
    "128": "extension_icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*"
      ],
      "js": [
        "js/vue.js",
        "js/app.js"
      ],
      "css": [
        "styles/style.css"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "*"
  ],
  "web_accessible_resources": [
    "fonts/Roboto-Regular.ttf",
    "styles/style.css",
    "images/edit-regular.svg"
  ]
}
